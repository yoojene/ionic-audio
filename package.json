    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "ionic-angular": "3.4.2",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12",
    "typescript": "2.3.4",
    "copyfiles": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arielfaur/ionic-audio.git"
  },
  "keywords": [
