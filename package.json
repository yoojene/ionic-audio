{
  "name": "ionic-audio",
  "version": "3.1.0",
  "description": "An audio player for Ionic 3 and Angular 4",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "node_modules/.bin/ngc && copyfiles -u 1 ./src/*.scss ./dist",
